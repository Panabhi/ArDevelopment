                <androidx.cardview.widget.CardView
                    android:id="@+id/cardvictoria"
                    android:layout_width="170dp"
                    android:layout_height="140dp"
                    android:layout_marginEnd="20dp"
                    app:cardCornerRadius="8dp"
                    app:cardElevation="5dp"
